#include ../libs/util.p8

bg = 6
fg = 5
sw = 84
sh = 48
sox = (128-sw)/2
soy = (128-sh)/2

function _update()
end

-- colors

function clear_screen()
    cls(0)
    rectfill(sox, soy, sox+sw-1, soy+sh-1, bg)
end

function _draw()
    clear_screen()

    ti = t()

    -- screen pos
    tx = sin(ti / 2) * 7 + 128/2 - 100*0.5
    tx = flr(tx)
    ty = cos(ti / 2) * 5 + 128/2 - 40*0.5
    ty = flr(ty)

    printc("hello nokia", 64-2, fg)

end

function _init()
end

